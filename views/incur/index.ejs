    <!-- header-start -->
    <%- include("./partials/incurHeader") %>
    <!-- header-end -->
    <div class="bradcam_area breadcam_bg overlay">
      <h3>WORLD'S FIRST VIRTUAL SKILL PARK</h3>
      <svg
        xmlns="http://www.w3.org/2000/svg"
        viewBox="0 0 1435 315"
